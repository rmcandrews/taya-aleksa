  "private": true,
  "dependencies": {
    "formik": "^2.1.5",
    "p5": "^1.1.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flexbox-grid": "^2.1.2",
    "react-headroom": "^3.0.0",
    "react-image-gallery": "^0.9.1",
    "react-photo-gallery": "^8.0.0",
    "react-progressive-graceful-image": "^0.6.12",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "react-select": "^3.1.0",
